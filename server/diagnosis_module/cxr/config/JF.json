{
  "DataRoot": "../data/mimic-cxr/",
  "ValPath": "./datasets/MIMIC-CXR/mimic_test_clean.json",
  "Data_CLASSES": [
    "Cardiomegaly",
    "Edema",
    "Consolidation",
    "Atelectasis",
    "Pleural Effusion"
  ],
  "backbone": "densenet121",
  "width": 512,
  "height": 512,
  "long_side": 512,
  "fix_ratio": true,
  "pixel_mean": 128.0,
  "pixel_std": 64.0,
  "use_pixel_std": true,
  "use_equalizeHist": true,
  "use_transforms_type": "Aug",
  "gaussian_blur": 3,
  "border_pad": "pixel_mean",
  "num_classes": [1, 1, 1, 1, 1],
  "batch_weight": true,
  "enhance_index": [2, 6],
  "enhance_times": 1,
  "pos_weight": [1, 1, 1, 1, 1],
  "pretrained": false,
  "log_every": 10,
  "test_every": 100,
  "epoch": 3,
  "norm_type": "BatchNorm",
  "global_pool": "AVG_MAX",
  "fc_bn": true,
  "attention_map": "FPA",
  "lse_gamma": 0.5,
  "fc_drop": 0,
  "optimizer": "Adam",
  "criterion": "BCE",
  "best_target": "auc",
  "save_top_k": 3,
  "save_index": [0, 1, 2, 3, 4],
  "num_workers": 8
}
